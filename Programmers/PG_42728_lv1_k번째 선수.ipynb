    "#O(nlogn) : 퀵소트, tim sort\n",
    "def solution(array, commands):\n",
    "    answer = [] \n",
    "    for command in commands :\n",
    "        i, j, k = command\n",
    "        answer.append(sorted(array[i-1:j])[k-1])\n",
    "    return answer"
